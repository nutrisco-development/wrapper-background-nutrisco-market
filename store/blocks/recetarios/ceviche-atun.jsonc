      "flex-layout.row#recipe-food-pattern1-call-to-action-2"
    ]
  },
  "flex-layout.row#recipe-food-pattern1-title": {
    "props": {
      "blockClass": ["StoreContainer", "SeparatorTop", "SeparatorBottom"],
      "fullWidth": false,
      "paddingTop": 2,
      "horizontalAlign": "center"
    },
    "children": ["rich-text#recipe-food-pattern1-title"]
  },
  "rich-text#recipe-food-pattern1-title": {
    "props": {
      "text": "# Ceviche de Atún \n*line*\n*line*\n*line*",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": ["section-title", "section-recipe-food"],
      "fullWidth": false,
      "marginTop": 0,
      "marginBottom": 0
    }
  },
  "flex-layout.row#recipe-food-pattern1-title2": {
    "props": {
      "blockClass": ["StoreContainer", "SeparatorTop", "SeparatorBottom"],
      "fullWidth": false,
      "paddingTop": 2,
      "horizontalAlign": "center"
    },
    "children": ["rich-text#recipe-food-pattern1-title2"]
  },
  "rich-text#recipe-food-pattern1-title2": {
    "props": {
      "text": "## Productos de Receta \n*line*\n*line*\n*line*",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": ["section-title", "section-recipe-food"],
      "fullWidth": false,
      "marginTop": 0,
      "marginBottom": 0
    }
  },
  "flex-layout.row#recipe-food-pattern1-subtitle": {
    "props": {
      "blockClass": ["StoreContainer", "SeparatorTop", "SeparatorBottom"],
      "fullWidth": false,
      "paddingTop": 0,
      "horizontalAlign": "center"
    },
    "children": ["rich-text#recipe-food-pattern1-subtitle"]
  },
  "rich-text#recipe-food-pattern1-subtitle": {
    "props": {
      "text": "## Esta receta te hará un experto en la cocina.",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": ["section-title", "short-description-recipe-food"],
      "fullWidth": false,
      "marginTop": 0,
      "marginBottom": 0
    }
  },
  "flex-layout.row#recipe-food-pattern1-call-to-action": {
    "props": {
      "blockClass": ["StoreContainer"],
      "fullWidth": false,
      "horizontalAlign": "center",
      "paddingTop": 5
    },
    "children": ["link#recipe-food-pattern1-call-to-action"]
  },
  "link#recipe-food-pattern1-call-to-action": {
    "props": {
      "blockClass": "link-recipe-food-pattern-call-to-action",
      "href": "/checkout/cart/add/?sku=54&qty=1&seller=1&sc=2&sku=58&qty=1&seller=1&sc=2&sku=59&qty=1&seller=1&sc=2&sku=75&qty=1&seller=1&sc=2&sku=79&qty=1&seller=1&sc=2&sku=98&qty=1&seller=1&sc=2",
      "label": "agregar productos al carrito"
    }
  },
  "flex-layout.row#recipe-food-pattern1-content": {
    "props": {
      "blockClass": ["StoreContainer"],
      "fullWidth": false,
      "paddingTop": 0,
      "horizontalAlign": "center"
    },
    "children": ["recipe-content-ceviche-atun"]
  },
  "flex-layout.row#shelf-recipe-food-pattern1": {
    "props": {
      "blockClass": ["featured-product-carousel", "StoreContainer", "StoreContainerRecipe"],
      "marginTop": 7,
      "marginBottom": 4
      //"paddingBottom": 10
    },
    "children": ["list-context.product-list#shelf-recipe-food-pattern1"]
  },
  "list-context.product-list#shelf-recipe-food-pattern1": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#shelf-recipe-food-pattern1"],
    "props": {
      "collection": "216",
      //"hideUnavailableItems": true,
      "maxItems": 10,
      "orderBy": "OrderByBestDiscountDESC"
    }
  },
  "slider-layout#shelf-recipe-food-pattern1": {
    "props": {
      "itemsPerPage": {
        "(min-width:1280px)": 6,
        "desktop": 3,
        "tablet": 2,
        "phone": 2
      },
      "infinite": true,
      "showNavigationArrows": "always",
      "showPaginationDots": "never",
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },
  "flex-layout.row#recipe-food-pattern1-call-to-action-2": {
    "props": {
      "blockClass": ["StoreContainer"],
      "fullWidth": false,
      "horizontalAlign": "center",
      "paddingTop": 5
    },
    "children": ["link#recipe-food-pattern1-call-to-action-2"]
  },
  "link#recipe-food-pattern1-call-to-action-2": {
    "props": {
      "blockClass": ["link-recipe-food-pattern-call-to-action", "link-recipe-food-pattern-call-to-action-2"],
      "href": "/recetarios",
      "label": "ir a recetarios"
    }
  }
