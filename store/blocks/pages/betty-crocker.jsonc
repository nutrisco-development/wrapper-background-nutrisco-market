{
  "store.custom#page-betty-crocker": {
    "blocks": ["search-result-layout.customQuery#page-betty-crocker"]
  },
  "search-result-layout.customQuery#page-betty-crocker": {
    "props": {
      "querySchema": {
        "orderByField": "OrderByBestDiscountDESC",
        // "hideUnavailableItems": true,
        "maxItemsPerPage": 10,
        "queryField": "betty-crocker",
        "mapField": "b",
        "skusFilter": "ALL_AVAILABLE"
      }
    },
    "blocks": [
      "search-result-layout.desktop#page-betty-crocker",
      "search-result-layout.mobile#page-betty-crocker",
      "search-not-found-layout"
    ]
  },
  "search-result-layout.desktop#page-betty-crocker": {
    "children": [
      "flex-layout.row#page-betty-crocker-banner",
      "flex-layout.row#searchtitle",
      "flex-layout.row#searchbread",
      "flex-layout.row#result"
    ],
    "props": {
      "pagination": "infinite",
      "defaultGalleryLayout": "grid",
      //pre
      "preventRouteChange": true
    }
  },
  "search-result-layout.mobile#page-betty-crocker": {
    "children": [
      "flex-layout.row#page-betty-crocker-banner",
      "flex-layout.row#searchinfomobile",
      "flex-layout.row#searchbread",
      "flex-layout.row#productCountMobile",
      "flex-layout.row#fetchprevious",
      "flex-layout.row#contentmobile",
      "flex-layout.row#fetchmore",
      "search-products-count-per-page"
    ],
    "props": {
      "blockClass": "mobileResults",
      "pagination": "infinite",
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      },
      "defaultGalleryLayout": "grid",
      //pre
      "preventRouteChange": true
    }
  },
  "flex-layout.row#page-betty-crocker-banner": {
    "props": {
      "blockClass": ["StoreContainer"],
      "marginTop": 4,
      "paddingBottom": 10,
      "colSizing": "auto",
      "fullWidth": true,
      "horizontalAlign": "center"
    },
    "children": ["list-context.image-list#page-betty-crocker-banner"]
  },
  "list-context.image-list#page-betty-crocker-banner": {
    "children": ["slider-layout#page-betty-crocker-banner"],
    "props": {
      "preload": true,
      "images": [
        {
          "image": "https://orizonb2ccl.vteximg.com.br/arquivos/TOP-BANNER-DESK-BRAND-BETTY-CROKER.jpg",
          "mobileImage": "https://orizonb2ccl.vteximg.com.br/arquivos/TOP-BANNER-MOB-BRAND-BETTY-CROKER.jpg"
        }
      ]
    }
  },
  "slider-layout#page-betty-crocker-banner": {
    "props": {
      "blockClass": ["ImageBannerPageCustom"],
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true
    }
  }
}
