  "search-result-layout.customQuery#page-petfood-desktop": {
    "props": {
      "querySchema": {
        "orderByField": "OrderByBestDiscountDESC",
        "maxItemsPerPage": 12,
        "queryField": "286",
        "mapField": "productClusterIds",
        "skusFilter": "ALL_AVAILABLE"
      }
    },
    "blocks": ["search-result-layout.desktop#page-petfood", "search-not-found-layout"]
  },
  "search-result-layout.desktop#page-petfood": {
    "children": [
      //"flex-layout.row#page-petfood-banner",
      //"flex-layout.row#search-title-custom-petfood",
      //"flex-layout.row#banner-slider-petfood",

      "flex-layout.row#searchbread",
      "flex-layout.row#result"
    ],
    "props": {
      "pagination": "infinite",
      "defaultGalleryLayout": "grid",
      "preventRouteChange": true,
      "blockClass": "TemplatePlpCustomgentil"
    }
  },
  "flex-layout.row#page-petfood-banner": {
    "props": {
      "marginBottom": 6,
      "fullWidth": true,
      "horizontalAlign": "center"
    },
    "children": ["list-context.image-list#page-petfood-banner"]
  },
  "list-context.image-list#page-petfood-banner": {
    "children": ["slider-layout#page-petfood-banner"],
    "props": {
      "preload": true,
      "images": [
        {
          "image": "https://orizonb2ccl.vteximg.com.br/arquivos/BANNER-MAIN-PAGE-PETFOOD-DESK-V2.jpg",
          "mobileImage": "https://orizonb2ccl.vteximg.com.br/arquivos/BANNER-MAIN-PAGE-PETFOOD-MOB-V2.jpg"
        }
      ]
    }
  },
  "slider-layout#page-petfood-banner": {
    "props": {
      // "blockClass": ["ImageBannerPageCustom"],
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true
    }
  },
  "image#image-slide-petfood": {
    "props": {
      "src": "https://orizonb2ccl.vtexassets.com/arquivos/BANNER-HOME-GENTIL-MARZO-21-COVER.jpg",
      "maxHeight": "430px"
    }
  },
  "search-result-layout.customQuery#page-petfood-mobile": {
    "props": {
      "querySchema": {
        "orderByField": "OrderByBestDiscountDESC",
        // "hideUnavailableItems": true,
        "maxItemsPerPage": 12,
        "queryField": "286",
        "mapField": "productClusterIds",
        "skusFilter": "ALL_AVAILABLE"
      }
    },
    "blocks": ["search-result-layout.mobile#page-petfood", "search-not-found-layout"]
  },
  "search-result-layout.mobile#page-petfood": {
    "children": [
      "flex-layout.row#headline-banner-carousel-petfood",
      "flex-layout.row#carousel-banner-petfood-desk",
      "flex-layout.row#searchinfomobile",
      "flex-layout.row#searchbread",
      "flex-layout.row#productCountMobile",
      "flex-layout.row#fetchprevious",
      "flex-layout.row#contentmobile",
      "flex-layout.row#fetchmore",
      "search-products-count-per-page"
    ],
    "props": {
      "blockClass": "TemplatePlpCustomgentil",
      "pagination": "infinite",
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      },
      "defaultGalleryLayout": "grid"
    }
  },

  "flex-layout.row#headline-banner-carousel-petfood": {
    "props": {
      "blockClass": ["StoreContainer", "HeadlineBannerCarousel"],
      "fullWidth": false
    },
    "children": ["rich-text#headline-banner-carousel-petfood"]
  },

  "rich-text#headline-banner-carousel-petfood": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "LEFT",
      "text": "Revisa todas nuestras ofertas",
      "blockClass": "HeadlineBannerCarousel"
    }
  },
  "flex-layout.row#carousel-banner-petfood-desk": {
    "children": ["list-context.product-list#carousel-banner-petfood-desk"]
  },
  "list-context.product-list#carousel-banner-petfood-desk": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#carousel-banner-petfood-desk"],
    "props": {
      "collection": "286",
      // "hideUnavailableItems": true,
      "maxItems": 10,
      "orderBy": "OrderByTopSaleDESC",
      "specificationFilters": []
    }
  },
  "slider-layout#carousel-banner-petfood-desk": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "showPaginationDots": "never",
      "fullWidth": false,
      "blockClass": "shelf"
    }
  }
}
