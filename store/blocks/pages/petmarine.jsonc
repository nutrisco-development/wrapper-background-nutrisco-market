{
  "store.custom#page-petmarine": {
    "blocks": [
      "responsive-layout.desktop#page-petmarine",
      "responsive-layout.tablet#page-petmarine",
      "responsive-layout.phone#page-petmarine",
      "search-result-layout.customQuery#page-petmarine-desktop"
    ]
  },
  "responsive-layout.desktop#page-petmarine": {
    //"children": ["flex-layout.row#page-petmarine-banner","flex-layout.row#banner-slider-petmarine"]
  },
  "responsive-layout.tablet#page-petmarine": {
    "children": [
      //"flex-layout.row#page-petmarine-banner",
      //"flex-layout.row#headline-banner-carousel-petmarine"
      // "flex-layout.row#carousel-banner-petmarine-desk"
    ]
  },
  "responsive-layout.phone#page-petmarine": {
    "children": [
      //"flex-layout.row#page-petmarine-banner",
      //"flex-layout.row#headline-banner-carousel-petmarine"
      //"flex-layout.row#carousel-banner-petmarine-desk"
    ]
  },
  ////
  "search-result-layout.customQuery#page-petmarine-desktop": {
    "props": {
      "preventRouteChange": true,
      "querySchema": {
        "orderByField": "OrderByBestDiscountDESC",
        //"hideUnavailableItems": true,
        "maxItemsPerPage": 12,
        "queryField": "292",
        "mapField": "productClusterIds",
        "skusFilter": "ALL_AVAILABLE"
      }
    },
    "blocks": ["search-result-layout.desktop#page-petmarine", "search-result-layout.mobile#page-petmarine", "search-not-found-layout"]
  },
  "search-result-layout.desktop#page-petmarine": {
    "children": ["flex-layout.row#search-title-custom-petmarine", "flex-layout.row#searchbread", "flex-layout.row#result"],
    "props": {
      "pagination": "infinite",
      "defaultGalleryLayout": "grid",
      //pre
      "preventRouteChange": true
    }
  },
  "search-result-layout.mobile#page-petmarine": {
    "children": [
      "flex-layout.row#searchinfomobile",
      "flex-layout.row#searchbread",
      "flex-layout.row#productCountMobile",
      "flex-layout.row#fetchprevious",
      "flex-layout.row#contentmobile",
      "flex-layout.row#fetchmore",
      "search-products-count-per-page"
    ],
    "props": {
      "blockClass": "mobileResults",
      "pagination": "infinite",
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      },
      "defaultGalleryLayout": "grid",
      //pre
      "preventRouteChange": true
    }
  },
  /////
  "flex-layout.row#page-petmarine-banner": {
    "props": {
      "blockClass": ["StoreContainer"],
      "marginTop": 4,
      "fullWidth": true,
      "horizontalAlign": "center"
    },
    "children": ["list-context.image-list#page-petmarine-banner1"]
  },
  "list-context.image-list#page-petmarine-banner1": {
    "children": ["slider-layout#page-petmarine-banner1"],
    "props": {
      "preload": true,
      "images": [
        {
          "image": "https://via.placeholder.com/1920x200/00000/FFFFFF/%20C/O%20https://placeholder.com/?text=Banner",
          "mobileImage": "https://via.placeholder.com/1025x350/00000/FFFFFF/%20C/O%20https://placeholder.com/?text=Banner"
        }
      ]
    }
  },
  "slider-layout#page-petmarine-banner1": {
    "props": {
      "blockClass": ["ImageBannerPageCustom"],
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true
    }
  },
  ///
  "flex-layout.row#banner-slider-petmarine": {
    "props": {
      "fullWidth": true,
      "blockClass": ["BannerCarruselCustom", "StoreContainer"],
      "paddingTop": 6
    },
    "children": ["flex-layout.col#image-slide-petmarine", "flex-layout.col#carrusel-slide-petmarine"]
  },

  "flex-layout.col#image-slide-petmarine": {
    "props": {
      "preventVerticalStretch": true,
      "verticalAlign": "middle",
      "marginRight": 1,
      "blockClass": "BannerCarruselCustom1"
    },
    "children": ["image#image-slide-petmarine1"]
  },

  "image#image-slide-petmarine1": {
    "props": {
      "src": "https://via.placeholder.com/430x430/000000/FFFFFF/%20C/O%20https://placeholder.com/?text=Cover",
      "maxHeight": "430px"
    }
  },

  "flex-layout.col#carrusel-slide-petmarine": {
    "props": {
      "width": "70%",
      "blockClass": "BannerCarruselCustomCol2"
    },
    "children": ["flex-layout.row#carousel-banner-petmarine-desk"]
  },
  ////
  "flex-layout.row#headline-banner-carousel-petmarine": {
    "props": {
      "blockClass": ["StoreContainer", "HeadlineBannerCarousel"],
      "fullWidth": false
    },
    "children": ["rich-text#headline-banner-carousel-petmarine"]
  },

  "rich-text#headline-banner-carousel-petmarine": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "LEFT",
      "text": "Revisa todas nuestras ofertas",
      "blockClass": "HeadlineBannerCarousel"
    }
  },
  ///
  "flex-layout.row#carousel-banner-petmarine-desk": {
    "children": ["list-context.product-list#carousel-banner-petmarine-desk"]
  },
  "list-context.product-list#carousel-banner-petmarine-desk": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#carousel-banner-petmarine-desk"],
    "props": {
      "collection": "292",
      // "orderBy": "OrderByTopSaleDESC",
      //"category": "142/146",
      //"hideUnavailableItems": true,
      "maxItems": 10,
      "orderBy": "OrderByTopSaleDESC",
      "specificationFilters": [],
      "skusFilter": "ALL_AVAILABLE"
    }
  },
  "slider-layout#carousel-banner-petmarine-desk": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "showPaginationDots": "never",
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },
  "flex-layout.row#search-title-custom-petmarine": {
    "props": {
      "blockClass": ["StoreContainer", "DynamicTitleCustom"]
    },
    "children": ["search-title.v2"]
  }
}
