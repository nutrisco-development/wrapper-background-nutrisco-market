{
  "store.custom#page-pets": {
    "blocks": [
      "responsive-layout.desktop#page-pets",
      "responsive-layout.tablet#page-pets",
      "responsive-layout.phone#page-pets",
      "search-result-layout.customQuery#page-pets-desktop",
      "pattern-background-home"
    ]
  },
  "responsive-layout.desktop#page-pets": {
    "children": ["flex-layout.row#page-pets-banner", "__fold__"]
  },
  "responsive-layout.tablet#page-pets": {
    "children": ["flex-layout.row#page-pets-banner", "__fold__"]
  },
  "responsive-layout.phone#page-pets": {
    "children": ["flex-layout.row#page-pets-banner", "__fold__"]
  },
  ////
  "search-result-layout.customQuery#page-pets-desktop": {
    "props": {
      "preventRouteChange": true,
      "querySchema": {
        "orderByField": "OrderByBestDiscountDESC",
        //"hideUnavailableItems": true,
        "maxItemsPerPage": 12,
        "queryField": "261",
        "mapField": "productClusterIds",
        "skusFilter": "ALL_AVAILABLE"
        /*
        "orderByField": "OrderByBestDiscountDESC",
        //"hideUnavailableItems": true,
        "maxItemsPerPage": 12,
        "queryField": "mascotas",
        "mapField": "c",
        "skusFilter": "ALL_AVAILABLE"*/
      }
    },
    "blocks": ["search-result-layout.desktop#page-pets", "search-result-layout.mobile#page-pets", "search-not-found-layout"]
  },
  "search-result-layout.desktop#page-pets": {
    "children": ["flex-layout.row#search-title-custom-pets", "flex-layout.row#searchbread", "flex-layout.row#result"],
    "props": {
      "pagination": "infinite",
      "defaultGalleryLayout": "grid",
      //pre
      "preventRouteChange": true,
      //"blockClass": "fixpets",
      "blockClass": "TemplatePlpCustomMascotas"
    }
  },
  "search-result-layout.mobile#page-pets": {
    "children": [
      "flex-layout.row#searchinfomobile",
      "flex-layout.row#searchbread",
      //"flex-layout.row#productCountMobile",
      "flex-layout.row#fetchprevious",
      "flex-layout.row#contentmobile",
      "flex-layout.row#fetchmore",
      "search-products-count-per-page"
    ],
    "props": {
      "blockClass": "TemplatePlpCustomMascotas",
      "pagination": "infinite",
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      },
      "defaultGalleryLayout": "grid",
      //pre
      "preventRouteChange": true
    }
  },
