  "flex-layout.row#page-pets-banner": {
    "props": {
      //"blockClass": ["StoreContainer"],
      //"marginTop": 4,
      "marginBottom": 8,
      "fullWidth": true,
      "horizontalAlign": "center"
    },
    "children": ["list-context.image-list#page-pets-banner12"]
  },
  "list-context.image-list#page-pets-banner12": {
    "children": ["slider-layout#page-pets-banner12"],
    "props": {
      "preload": true,
      "images": [
        {
          //"image": "https://i.imgur.com/61bIlxw.jpg",
          "image": "https://orizonb2ccl.vteximg.com.br/arquivos/BANNER-PETS-2023-DESK.jpg",
          "mobileImage": "https://orizonb2ccl.vteximg.com.br/arquivos/BANNER-PETS-2023-MOB.jpg"
        }
      ]
    }
  },
  "slider-layout#page-pets-banner12": {
    "props": {
      "blockClass": ["ImageBannerPageCustom", "ImageBannerPageCustomBrandpets"],
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true
    }
  },
