  "flex-layout.row#banner-slider-pets": {
    "props": {
      "fullWidth": true,
      "blockClass": ["BannerCarruselCustom", "StoreContainer"],
      "paddingTop": 6
    },
    "children": ["flex-layout.col#image-slide-pets", "flex-layout.col#carrusel-slide-pets"]
  },

  "flex-layout.col#image-slide-pets": {
    "props": {
      "preventVerticalStretch": true,
      "verticalAlign": "middle",
      "marginRight": 1,
      "blockClass": "BannerCarruselCustom1"
    },
    "children": ["image#image-slide-pets1"]
  },

  "image#image-slide-pets1": {
    "props": {
      "src": "https://orizonb2ccl.vteximg.com.br/arquivos/BANNER-PETS-PLP-COVER-2-LMTD-2022.jpg",
      "maxHeight": "430px"
    }
  },

  "flex-layout.col#carrusel-slide-pets": {
    "props": {
      "width": "70%",
      "blockClass": ["BannerCarruselCustomCol2", "SliderBrandpets"]
    },
    "children": ["flex-layout.row#carousel-banner-pets-desk"]
  },
  ////
  "flex-layout.row#headline-banner-carousel-pets": {
    "props": {
      "blockClass": ["StoreContainer", "HeadlineBannerCarousel"],
      "fullWidth": false
    },
    "children": ["rich-text#headline-banner-carousel-pets"]
  },

  "rich-text#headline-banner-carousel-pets": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "üê∂ La Mesa de las Mascotas üò∫",
      "blockClass": "HeadlineBannerCarousel"
