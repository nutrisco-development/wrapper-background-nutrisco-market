  "flex-layout.row#carousel-banner-pets-desk": {
    "children": ["list-context.product-list#carousel-banner-pets-desk"]
  },
  "list-context.product-list#carousel-banner-pets-desk": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#carousel-banner-pets-desk"],
    "props": {
      "collection": "261",
      // "orderBy": "OrderByTopSaleDESC",
      //"category": "142/146",
      //"hideUnavailableItems": true,
      "maxItems": 10,
      "orderBy": "OrderByTopSaleDESC",
      "specificationFilters": [],
      "skusFilter": "ALL_AVAILABLE"
    }
  },
  "slider-layout#carousel-banner-pets-desk": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "showPaginationDots": "never",
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },
  "flex-layout.row#search-title-custom-pets": {
    "props": {
      "blockClass": ["StoreContainer", "DynamicTitleCustom"]
    },
    "children": ["search-title.v2"]
  }
