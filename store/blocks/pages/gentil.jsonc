  "flex-layout.row#banner-slider-gentil": {
    "props": {
      "fullWidth": true,
      "blockClass": ["BannerCarruselCustom", "StoreContainer"],
      "paddingTop": 6
    },
    "children": ["flex-layout.col#image-slide-gentil", "flex-layout.col#carrusel-slide-gentil"]
  },

  "flex-layout.col#image-slide-gentil": {
    "props": {
      "preventVerticalStretch": true,
      "verticalAlign": "middle",
      "marginRight": 1,
      "blockClass": "BannerCarruselCustom1"
    },
    "children": ["image#image-slide-gentil1"]
  },

  "image#image-slide-gentil1": {
    "props": {
      "src": "https://orizonb2ccl.vteximg.com.br/arquivos/BANNER-HOME-GENTIL-MARZO-21-COVER.jpg",
      "maxHeight": "430px"
    }
  },

  "flex-layout.col#carrusel-slide-gentil": {
    "props": {
      "width": "70%",
      "blockClass": ["BannerCarruselCustomCol2", "SliderBrandgentil"]
    },
    "children": ["flex-layout.row#carousel-banner-gentil-desk"]
  },
  ////
  "flex-layout.row#headline-banner-carousel-gentil": {
    "props": {
      "blockClass": ["StoreContainer", "HeadlineBannerCarousel"],
      "fullWidth": false
    },
    "children": ["rich-text#headline-banner-carousel-gentil"]
  },

  "rich-text#headline-banner-carousel-gentil": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "LEFT",
      "text": "Revisa todas nuestras ofertas",
      "blockClass": "HeadlineBannerCarousel"
    }
  },
  ///
  "flex-layout.row#carousel-banner-gentil-desk": {
    "children": ["list-context.product-list#carousel-banner-gentil-desk"]
  },
  "list-context.product-list#carousel-banner-gentil-desk": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#carousel-banner-gentil-desk"],
    "props": {
      "collection": "280",
      "maxItems": 10,
      "orderBy": "OrderByTopSaleDESC",
      "specificationFilters": [],
      "skusFilter": "ALL_AVAILABLE"
      // "orderBy": "OrderByTopSaleDESC",
      // "category": "142/146",
      //"hideUnavailableItems": true,
    }
  },
  "slider-layout#carousel-banner-gentil-desk": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "showPaginationDots": "never",
      "fullWidth": false,
      "blockClass": "shelf"
    }
  },
  "flex-layout.row#search-title-custom-gentil": {
    "props": {
      "blockClass": ["StoreContainer", "DynamicTitleCustom"]
    },
    "children": ["search-title.v2"]
  }
