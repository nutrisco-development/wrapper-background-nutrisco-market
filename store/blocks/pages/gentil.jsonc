{
  "store.custom#page-gentil": {
    "blocks": [
      "responsive-layout.desktop#page-gentil",
      "responsive-layout.tablet#page-gentil",
      "responsive-layout.phone#page-gentil",
      "search-result-layout.customQuery#page-gentil-desktop",
      "pattern-background-home"
    ]
  },
  "responsive-layout.desktop#page-gentil": {
    //"children": ["flex-layout.row#page-gentil-banner", "flex-layout.row#banner-slider-gentil", "__fold__"]
    "children": ["flex-layout.row#page-gentil-banner", "__fold__"]
  },
  "responsive-layout.tablet#page-gentil": {
    // "children": [
    //   "flex-layout.row#page-gentil-banner",
    //   "flex-layout.row#headline-banner-carousel-gentil",
    //   "flex-layout.row#carousel-banner-gentil-desk",
    //   "__fold__"
    // ]
    "children": ["flex-layout.row#page-gentil-banner", "__fold__"]
  },
  "responsive-layout.phone#page-gentil": {
    // "children": [
    //   "flex-layout.row#page-gentil-banner",
    //   "flex-layout.row#headline-banner-carousel-gentil",
    //   "flex-layout.row#carousel-banner-gentil-desk",
    //   "__fold__"
    // ]
    "children": ["flex-layout.row#page-gentil-banner", "__fold__"]
  },
  ////
  "search-result-layout.customQuery#page-gentil-desktop": {
    "props": {
      "preventRouteChange": true,
      "querySchema": {
        "orderByField": "OrderByBestDiscountDESC",
        // "hideUnavailableItems": true,
        "maxItemsPerPage": 12,
        // "queryField": "263",
        // "mapField": "productClusterIds",
        "skusFilter": "ALL_AVAILABLE",
        "queryField": "gentil",
        "mapField": "b"
      }
    },
    "blocks": ["search-result-layout.desktop#page-gentil", "search-result-layout.mobile#page-gentil", "search-not-found-layout"]
  },
  "search-result-layout.desktop#page-gentil": {
    "children": ["flex-layout.row#search-title-custom-gentil", "flex-layout.row#searchbread", "flex-layout.row#result"],
    "props": {
      "pagination": "infinite",
      "defaultGalleryLayout": "grid",
      //pre
      "preventRouteChange": true,
      "blockClass": "TemplatePlpCustomgentil"
    }
  },
  "search-result-layout.mobile#page-gentil": {
    "children": [
      "flex-layout.row#searchinfomobile",
      "flex-layout.row#searchbread",
      //"flex-layout.row#productCountMobile",
      "flex-layout.row#fetchprevious",
      "flex-layout.row#contentmobile",
      "flex-layout.row#fetchmore",
      "search-products-count-per-page"
    ],
    "props": {
      "blockClass": "TemplatePlpCustomgentil",
      "pagination": "infinite",
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      },
      "defaultGalleryLayout": "grid",
      //pre
      "preventRouteChange": true
    }
  },
  /////
  "flex-layout.row#page-gentil-banner": {
    "props": {
      //"blockClass": ["StoreContainer"],
      //"marginTop": 4,
      "marginBottom": 8,
      "fullWidth": true,
      "horizontalAlign": "center"
    },
    "children": ["list-context.image-list#page-gentil-banner1"]
  },
  "list-context.image-list#page-gentil-banner1": {
    "children": ["slider-layout#page-gentil-banner1"],
    "props": {
      "preload": true,
      "images": [
        {
          "image": "https://orizonb2ccl.vteximg.com.br/arquivos/BANNER-HOME-GENTIL-MARZO-21-DESK-2.png",
          "mobileImage": "https://orizonb2ccl.vteximg.com.br/arquivos/BANNER-HOME-GENTIL-MARZO-21-MOB-2.png"
        }
      ]
    }
  },
  "slider-layout#page-gentil-banner1": {
    "props": {
      "blockClass": ["ImageBannerPageCustom", "ImageBannerPageCustomBrandgentil"],
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true
    }
  },
