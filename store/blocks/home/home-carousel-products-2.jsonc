{
  // #1
  "flex-layout.row#CarouselCategory-2": {
    "props": {
      "fullWidth": true,
      "blockClass": ["BannerCarruselCustom", "StoreContainer"]
    },
    "children": ["flex-layout.col#CarouselCategory-2-Image", "flex-layout.col#CarouselCategory-2-Slider"]
  },
  "flex-layout.col#CarouselCategory-2-Image": {
    "props": {
      "preventVerticalStretch": true,
      "verticalAlign": "top",
      "marginRight": 1,
      "blockClass": "BannerCarruselCustom1"
    },
    "children": ["rich-text#CarouselCategory-2-ImageTitle", "image#CarouselCategory-2-Image"]
  },
  "rich-text#CarouselCategory-2-ImageTitle": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      //"text": "Helados",
      "text": "[Helados](/congelados/helados)",
      "blockClass": ["CarouselCategoryImageTitle", "BgLightBlueBrand"]
    }
  },
  "image#CarouselCategory-2-Image": {
    "props": {
      "src": "https://orizonb2ccl.vtexassets.com/arquivos/HomeCarouselProduct2.png",
      "maxHeight": "430px",
      "link": {
        "url": "/congelados/helados",
        "attributeTitle": "Helados",
        "newTab": false,
        "attributeNofollow": false
      }
    }
  },
  "flex-layout.col#CarouselCategory-2-Slider": {
    "props": {
      "width": "70%",
      "blockClass": "BannerCarruselCustomCol2"
    },
    "children": ["flex-layout.row#CarouselCategory-2-Slider"]
  },
  "flex-layout.row#CarouselCategory-2-Slider": {
    "children": ["list-context.product-list#CarouselCategory-2-Slider"]
  },
  "list-context.product-list#CarouselCategory-2-Slider": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#CarouselCategory-2-Slider"],
    "props": {
      "category": "142/146",
      //"hideUnavailableItems": true,
      "maxItems": 10,
      "orderBy": "OrderByTopSaleDESC",
      "specificationFilters": [],
      "skusFilter": "ALL_AVAILABLE"
    }
  },
  "slider-layout#CarouselCategory-2-Slider": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "showPaginationDots": "never",
      "fullWidth": false,
      "blockClass": "shelf"
    }
  }

  ///
}
